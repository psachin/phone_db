body {
   font-family: "trebuchet ms", verdana, sans-serif;
   font-size: 12px;
   line-height: 1.5em;
   color: #333;
   background: #ffffff;
   margin: 0;
   padding: 0;
   text-align: center;
   width: 100%;
}
#header {
   position: absolute;
   top: 0px;
   left: 0px;
   right: 0px;
   height: 60px;
   width: 100%;
   background: #333;
   padding-top: 8px;
}
#header h1 {
   font-size: 30px;
   text-transform: uppercase;
   letter-spacing: 0.3em;
   color: #fff;
}
#menu {
      font-family: "trebuchet ms", verdana, sans-serif;
      font-size: 13px;
      font-weight: bold;
      position: absolute;
      height: 27px;
      top: 60px;
      left: 0px;
      width: 100%;
      padding: 0px;
      color: #000000;
      background-color: #eee;
}
#container {
     position: absolute;
     top: 90px;
     left: 0px;
     right: 0px;
     background: #fff;
     margin: 0 auto 0 auto;
     text-align: left;
     width: 100%;
     height: 100%;
}
#bar {
     float: left;
     width: 200px;
     background: #eee;
     padding: 10px;
     margin-right: 30px;
     height: 100%;
}
#bar h1 {
        font-size: 12px;
        text-transform: uppercase;
        letter-spacing: 0.3em;
}
#bar_r {
     float: right;
     width: 200px;
     background:#eee;
     padding: 10px;
     margin-right: 0px;
     height: 100%;
}
#bar_r h1 {
        font-size: 12px;
        text-transform: uppercase;
        letter-spacing: 0.3em;
}
#main {
      margin: 15px 240px 15px 240px;
      padding: 15px 15px 15px 15px;
      background: #fff;
      border-style:dashed;
      border-color:#919096;
      border-width:thin;
      height:auto;
}
#img {
    border-width: medium;
    border-style:solid;
}
table {
   border: thin solid #cccccc;
   background: #ffffff;
}
th {
   letter-spacing: 2.0px;
   background: #eeeeeee;
   color: #000000;
   text-transform: uppercase;
   text-align: center;
   border-top: thick solid #eeeeee;
   border-bottom: thin solid #cccccc;
}
#base {
    position: obsolute;
    bottom: 0px;
    left: 0px;
    right:0px;
    height: 20px;
    width: 100%;
    background: #B3B2B9;
}
#base h1 {
    text-align:center;
    font-size: 10px;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    color: #fff;
}
a:link {
   text-decoration: none;
   color: #67676B;
   text-transform:none;
   cursor:default;
}
a:visited {
   text-decoration: none;
   border-bottom: 1px dotted #369;
   color: magenta;
}
a:hover, a:active {
    font-weight:bold;
    text-decoration: none;
    text-transform:uppercase;
/*     border-bottom: 1px solid #036; */
/*     border-color:blue; */
    color: #000;
}
p {
  text-align: justify;
}
